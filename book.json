{
    "title": "文档库",
    "root": "./docs",
    "plugins": [
        "expandable-chapters",
        "copy-code-button",
        "simple-page-toc",
        "prism",
        "prism-themes",
        "-highlight",
        "-lunr",
        "-search",
        "search-plus"
    ],
    "pluginsConfig": {
        "simple-page-toc": {
            "maxDepth": 4,
            "skipFirstH1": true
        },
        "prism": {
            "css": [
                "prism-themes/themes/prism-material-dark.css"
            ]
        }
    }
}